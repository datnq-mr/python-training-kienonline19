{% extends "base.jinja2" %}

{% block content %}

<form method="POST">
    <div>
        <label for="title">Title</label>
        <input type="text" id="title" name="title">
    </div>

    <div>
        <label for="content">Content</label>
        <textarea id="content" name="content"></textarea>
    </div>

    <div>
        <input type="submit">
    </div>
</form>

{% endblock  %}
